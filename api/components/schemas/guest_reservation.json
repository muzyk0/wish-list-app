{
  "type": "object",
  "description": "Reservation request for unauthenticated guests",
  "required": [
    "guest_name",
    "guest_email"
  ],
  "properties": {
    "guest_name": {
      "type": "string",
      "maxLength": 200,
      "description": "Name of the guest"
    },
    "guest_email": {
      "type": "string",
      "format": "email",
      "description": "Email of the guest"
    }
  }
}