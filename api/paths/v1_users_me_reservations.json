{
  "get": {
    "summary": "Get all reservations made by the authenticated user",
    "tags": [
      "Reservations"
    ],
    "security": [
      {
        "BearerAuth": []
      }
    ],
    "parameters": [
      {
        "name": "page",
        "in": "query",
        "schema": {
          "type": "integer",
          "minimum": 1,
          "default": 1
        }
      },
      {
        "name": "limit",
        "in": "query",
        "schema": {
          "type": "integer",
          "minimum": 1,
          "maximum": 100,
          "default": 10
        }
      }
    ],
    "responses": {
      "200": {
        "description": "List of user's reservations",
        "content": {
          "application/json": {
            "schema": {
              "type": "object",
              "properties": {
                "data": {
                  "type": "array",
                  "items": {
                    "$ref": "../components/schemas/reservation_details_response.json"
                  }
                },
                "pagination": {
                  "$ref": "../components/schemas/pagination.json"
                }
              }
            }
          }
        }
      },
      "401": {
        "$ref": "../components/responses/Unauthorized.json"
      }
    }
  }
}